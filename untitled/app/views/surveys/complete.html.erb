<nav>
  <div class="nav-wrapper">
    <a class="brand-logo center">Welcome</a>
    <ul id="nav-mobile" class="left hide-on-med-and-down">
    </ul>
  </div>
</nav>


<div class="notice"><%= flash[:notice] %></div>

<div class="row"></div>
<div class="row"></div>
<div class=" large">
  <div class="card-content">
    <div class="row">
      <div class="col s6">
        <a href="/" class="btn waves-effect waves-light" type="submit" name="action">
          <i class="material-icons">Return Home</i>
        </a>

      </div>
      <div class="col s4"> Total times done <%=	 @surveys.count %></div>
    </div>
    <table class="highlight centered responsive-table">
      <thead>
      <tr>
        <th data-field="name">Name</th>
        <th data-field="location">Location</th>
        <th data-field="favorite_Language">Favorite Language</th>
        <th data-field="comment">Comment</th>
        <th data-field="action">Action</th>
      </tr>
      </thead>
      <tbody>
      <%	@surveys.each do |survey|  %>
          <tr>
            <td><%= survey.full_name  %></td>
            <td> <%= survey.location %></td>
            <td><%= survey.favorite %></td>
            <td><%= survey.comment %></td>
            <td><a href="/delete<%= survey.id %>" class="btn waves-effect waves-light red">Delete</a></td>
          </tr>
      <%	end  %>
      </tbody>
    </table>
  </div>
</div>