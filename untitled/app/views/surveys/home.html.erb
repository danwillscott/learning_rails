<nav>
  <div class="nav-wrapper">
    <a class="brand-logo center">Welcome</a>
    <ul id="nav-mobile" class="left hide-on-med-and-down">
    </ul>
  </div>
</nav>
<div class="row"></div>
<div class="card large">
  <div class="card-content">
    <div class="row">
      <form method="post" action="/add" class="col s12">
        <input name="authenticity_token" value="<%= form_authenticity_token %>" type="hidden">
        <div class="row">
          <div class="input-field col s6">
            <input placeholder="Name" name="full_name" type="text">
            <label for="name"></label>
          </div>
          <div class="input-field col s4">
          </div>
        </div>
        <div class="row">
          <div class="input-field col s4">
            <select name="location" class="browser-default">
              <option value="" disabled selected>Location</option>
              <option value="Dallas">Dallas</option>
              <option value="San Jose">San Jose</option>
              <option value="Seattle">Seattle</option>
            </select>
          </div>
        </div>
        <div class="row">
          <div class="input-field col s4">
          <select name="favorite" class="browser-default">
            <option value="" disabled selected>Favorite Language</option>
            <option value="Ruby">Ruby</option>
            <option value="Java Script">Java Script</option>
            <option value="Python">Python</option>
          </select>
          </div>
        </div>
        <div class="row">
          <div class="input-field col s6">
            <label for="comment"></label>
            <textarea placeholder="comment" name="comment" class="materialize-textarea"></textarea>
          </div>
        </div>
        <button class="btn waves-effect waves-light" type="submit" name="action">
          <i class="material-icons">Submit</i>
        </button>
        <div class="center">
        </div>
      </form>
    </div>
  </div>
</div>



